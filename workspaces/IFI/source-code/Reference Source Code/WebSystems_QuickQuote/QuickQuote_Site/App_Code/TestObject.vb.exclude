Imports Microsoft.VisualBasic

<Serializable()> _
Public Class TestObject '*added 3/5/2013 for testing arbitrary fields in xml
    Implements IDisposable

    Dim qqHelper As New QuickQuoteHelperClass

    Private _Description As String
    Private _TestFlag As Boolean
    Private _TestDate As String

    Public Property Description As String
        Get
            Return _Description
        End Get
        Set(value As String)
            _Description = value
        End Set
    End Property
    Public Property TestFlag As Boolean
        Get
            Return _TestFlag
        End Get
        Set(value As Boolean)
            _TestFlag = value
        End Set
    End Property
    Public Property TestDate As String
        Get
            Return _TestDate
        End Get
        Set(value As String)
            _TestDate = value
            qqHelper.ConvertToShortDate(_TestDate)
        End Set
    End Property

#Region "IDisposable Support"
    Private disposedValue As Boolean ' To detect redundant calls

    ' IDisposable
    Protected Overridable Sub Dispose(disposing As Boolean)
        If Not Me.disposedValue Then
            If disposing Then
                ' TODO: dispose managed state (managed objects).
                If _Description IsNot Nothing Then
                    _Description = Nothing
                End If
                If _TestFlag <> Nothing Then
                    _TestFlag = Nothing
                End If
                If _TestDate IsNot Nothing Then
                    _TestDate = Nothing
                End If
            End If

            ' TODO: free unmanaged resources (unmanaged objects) and override Finalize() below.
            ' TODO: set large fields to null.
        End If
        Me.disposedValue = True
    End Sub

    ' TODO: override Finalize() only if Dispose(ByVal disposing As Boolean) above has code to free unmanaged resources.
    'Protected Overrides Sub Finalize()
    '    ' Do not change this code.  Put cleanup code in Dispose(ByVal disposing As Boolean) above.
    '    Dispose(False)
    '    MyBase.Finalize()
    'End Sub

    ' This code added by Visual Basic to correctly implement the disposable pattern.
    Public Sub Dispose() Implements IDisposable.Dispose
        ' Do not change this code.  Put cleanup code in Dispose(ByVal disposing As Boolean) above.
        Dispose(True)
        GC.SuppressFinalize(Me)
    End Sub
#End Region

End Class
